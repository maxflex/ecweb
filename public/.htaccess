<IfModule mod_rewrite.c>
    <IfModule mod_negotiation.c>
        Options -MultiViews
    </IfModule>

    RewriteEngine On

    # Redirect .web subdomain
    # RewriteCond %{HTTP_HOST} ^web\.ege-repetitor\.ru
    # RewriteRule ^(.*)$ https://ege-repetitor.ru/$1 [L]

    # Redirect Trailing Slashes If Not A Folder...
    # RewriteCond %{REQUEST_FILENAME} !-d
    # RewriteRule ^(.*)/$ /$1 [L,R=301]

    # Handle Front Controller...
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteRule ^ index.php [L]

    # Handle Authorization Header
    RewriteCond %{HTTP:Authorization} .
    RewriteRule .* - [E=HTTP_AUTHORIZATION:%{HTTP:Authorization}]


     Redirect 301 /licence/ https://ege-centr.ru/license/
     RedirectMatch 301 ^/price/.*$ https://ege-centr.ru/pricing/
     Redirect 301 /address/201/ https://ege-centr.ru/address/trg/

     RedirectMatch 301 /courses/11/(.*) https://ege-centr.ru/courses/ege/$1
     RedirectMatch 301 /courses/9/(.*) https://ege-centr.ru/courses/oge/$1
     Redirect 301 /opinions/ https://ege-centr.ru/reviews/
</IfModule>
